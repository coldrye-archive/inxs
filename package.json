{
  "name": "inxs",
  "version": "0.0.7",
  "description": "Decorator based dependency injection for Babel (node, browser)",
  "keywords": ["decorator", "dependency injection", "framework"],
  "license": "Apache-2.0",
  "homepage": "http://inxs.coldrye.eu/",
  "bugs": {
    "url": "https://github.com/coldrye-es/inxs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coldrye-es/inxs.git"
  },
  "author": {
    "name": "Carsten Klein",
    "email": "silkentrance@gmail.com"
  },
  "maintainers": [
    {
      "name": "cklein",
      "email": "silkentrance@gmail.com"
    }
  ],
  "main": "lib/inxs.js",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "inxs-common": "inxs-common",
    "sprintf-js": "^1.0.3"
  },
  "devDependencies": {
    "esaver": "esaver"
  },
  "globalDevDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.1.1",
    "babel-istanbul": "^0.3.20",
    "esdoc": "^0.3.1",
    "eslint": "^1.3.1",
    "eslint-config-coldrye": "eslint-config-coldrye",
    "mocha": "^2.2.5"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "externalHelpers": true,
    "optional": [
      "optimisation.flow.forOf",
      "runtime",
      "es7.decorators"
    ],
    "env": {
      "TEST": {
        "auxiliaryCommentBefore": "istanbul ignore next",
        "ignore": false
      },
      "BUILD": {
        "compact": false
      },
      "DIST": {
        "compact": true,
        "comments": false
      }
    }
  }
}
